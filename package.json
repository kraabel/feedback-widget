{
  "name": "@kraabelai/feedback-widget",
  "version": "1.0.0",
  "description": "A standalone bug tracking and feedback widget with screenshot capture, annotation tools, and admin dashboard",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./src/styles/base.css",
    "./server": {
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src/styles",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "dependencies": {
    "html-to-image": "^1.11.11",
    "lucide-react": "^0.400.0",
    "date-fns": "^3.0.0",
    "zod": "^3.22.0",
    "clsx": "^2.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "typescript": "^5.0.0",
    "tsup": "^8.0.0",
    "tailwindcss": "^3.4.0"
  },
  "keywords": [
    "feedback",
    "bug-tracker",
    "screenshot",
    "annotation",
    "widget",
    "react",
    "florence",
    "lms"
  ],
  "author": "BAYADA College of Nursing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bayada/feedback-widget"
  }
}
